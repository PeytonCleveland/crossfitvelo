{
  "name": "@crossfit-velo/db",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "npm run with-env prisma generate",
    "db:push": "npm run with-env prisma db push --skip-generate",
    "studio": "npm run with-env prisma studio --port 5556",
    "format": "prisma format && prettier --check \"**/*.{mjs,ts,json}\"",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "kysely": "^0.27.2",
    "nanoid": "^5.0.4",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@crossfit-velo/eslint-config": "^0.2.0",
    "@crossfit-velo/prettier-config": "^0.1.0",
    "@crossfit-velo/tsconfig": "^0.1.0",
    "@types/pg": "^8.11.5",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "prisma-kysely": "^1.7.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "@crossfit-velo/eslint-config/base"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-definitions": "off"
    }
  },
  "prettier": "@crossfit-velo/prettier-config"
}
